{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Gami Plugin",
  "type": "object",
  "properties": {
    "key": {
      "type": "string",
      "description": "The person's first name."
    },
    "name": {
      "type": "string",
      "description": "The person's last name."
    },
    "age": {
      "description": "Age in years which must be equal to or greater than zero.",
      "type": "integer",
      "minimum": 0
    }
  }
}
